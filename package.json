    "build-only": "vite build",
    "prettier:fix": "prettier --write src/",
    "test:prettier": "prettier --check src/",
    "lint:fix": "eslint src/ --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "test:eslint": "eslint src/",
    "test:type-check": "vue-tsc --noEmit --skipLibCheck -p tsconfig.vitest.json --composite false",
    "test:stylelint": "stylelint \"**/*.{vue,css}\"",
    "test:unit": "vitest",
    "test:unit-once": "vitest --watch=false",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.{vue,ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "**/*.{vue,css}": [
